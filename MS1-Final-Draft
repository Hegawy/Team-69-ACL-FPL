   "source": [
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "import pandas as pd\n",
    "\n",
    "# Filter season\n",
    "season_2023 = df[df['season_x'] == '2022-23'].copy()\n",
    "\n",
    "# ‚îÄ‚îÄ Top 5 players by total points ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ\n",
    "top5_total = (\n",
    "    season_2023.groupby('name')['total_points']\n",
    "    .sum()\n",
    "    .sort_values(ascending=False)\n",
    "    .head(5)\n",
    ")\n",
    "\n",
    "# ‚îÄ‚îÄ Top 5 players by highest form (peak form) ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ\n",
    "top5_form_peak = (\n",
    "    season_2023.groupby('name')['form']\n",
    "    .max()\n",
    "    .sort_values(ascending=False)\n",
    "    .head(5)\n",
    ")\n",
    "\n",
    "# ‚îÄ‚îÄ Plot side-by-side figures ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ\n",
    "fig, axes = plt.subplots(1, 2, figsize=(14, 6))\n",
    "\n",
    "# --- Chart 1 : Total Points ---\n",
    "sns.barplot(\n",
    "    x=top5_total.values, y=top5_total.index,\n",
    "    ax=axes[0], palette='Blues_r'\n",
    ")\n",
    "axes[0].set_title('Top 5 Players by Total Points (2022‚Äì23)')\n",
    "axes[0].set_xlabel('Total Points')\n",
    "axes[0].set_ylabel('Player')\n",
    "axes[0].grid(True, axis='x', linestyle='--', alpha=0.4)\n",
    "\n",
    "# --- Chart 2 : Highest Form ---\n",
    "sns.barplot(\n",
    "    x=top5_form_peak.values, y=top5_form_peak.index,\n",
    "    ax=axes[1], palette='Greens_r'\n",
    ")\n",
    "axes[1].set_title('Top 5 Players by Highest Form (2022‚Äì23)')\n",
    "axes[1].set_xlabel('Highest Form (Peak)')\n",
    "axes[1].set_ylabel('Player')\n",
    "axes[1].grid(True, axis='x', linestyle='--', alpha=0.4)\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()\n"
   ]
