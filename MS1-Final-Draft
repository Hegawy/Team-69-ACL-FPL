       "    document.getElementById('iWAJI9VM62T0UN8BJJ3IB')\n",
       "  );\n",
       "</script>"
      ],
      "text/plain": [
       "<shap.plots._force.AdditiveForceVisualizer at 0x7cadb7b35c90>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "# ============================\n",
    "# 2) SHAP setup ‚Äî TURBO (fast & per-position sampling)\n",
    "# ============================\n",
    "import shap, numpy as np, matplotlib.pyplot as plt\n",
    "from sklearn.utils import check_random_state\n",
    "import shap\n",
    "shap.initjs()  # load SHAP‚Äôs JS assets for interactive force plots\n",
    "\n",
    "\n",
    "rng = check_random_state(42)\n",
    "\n",
    "# Use a SMALL background to speed things up\n",
    "BG_N = min(100, len(X_train_s))\n",
    "bg_idx = rng.choice(len(X_train_s), size=BG_N, replace=False)\n",
    "background_all = X_train_s[bg_idx]\n",
    "\n",
    "# Try fast, gradient-based explainer first; else fall back to permutation with caps\n",
    "explainer_all = None\n",
    "explainer_kind = None\n",
    "\n",
    "try:\n",
    "    # GradientExplainer is usually the best bet for TF2/Keras models\n",
    "    explainer_all = shap.GradientExplainer(ffnn, background_all)\n",
    "    # quick probe\n",
    "    _ = explainer_all.shap_values(X_test_s[:4])\n",
    "    explainer_kind = \"GradientExplainer\"\n",
    "except Exception as e1:\n",
    "    try:\n",
    "        # PermutationExplainer via generic dispatcher with a capped budget\n",
    "        masker = shap.maskers.Independent(background_all)\n",
    "        explainer_all = shap.Explainer(\n",
    "            ffnn, masker=masker, algorithm=\"permutation\", max_evals=2 * X_train_s.shape[1] + 256\n",
    "        )\n",
    "        _ = explainer_all(X_test_s[:4])  # probe\n",
    "        explainer_kind = \"PermutationExplainer\"\n",
    "    except Exception as e2:\n",
    "        # Final safety: KernelExplainer but on a tiny background (still capped later by sampling)\n",
    "        explainer_all = shap.KernelExplainer(ffnn.predict, background_all)\n",
    "        explainer_kind = \"KernelExplainer\"\n",
    "\n",
    "print(f\"Using SHAP: {explainer_kind}\")\n",
    "\n",
    "def compute_shap_values_fast(X_pos_s):\n",
    "    \"\"\"\n",
    "    Fast SHAP with graceful handling of different explainers.\n",
    "    Returns (shap_matrix, base_value_scalar).\n",
    "    \"\"\"\n",
    "    if explainer_kind == \"GradientExplainer\":\n",
    "        shap_vals_list = explainer_all.shap_values(X_pos_s)\n",
    "        # GradientExplainer returns a list; regression -> single output\n",
    "        shap_vals_arr = np.array(shap_vals_list[0]) if isinstance(shap_vals_list, (list, tuple)) else np.array(shap_vals_list)\n",
    "        base_scalar = float(ffnn.predict(background_all).mean())\n",
    "        return shap_vals_arr, base_scalar\n",
    "\n",
    "    if explainer_kind == \"PermutationExplainer\":\n",
    "        exp = explainer_all(X_pos_s)  # Explanation\n",
    "        shap_vals_arr = np.array(exp.values)\n",
    "        # robust base\n",
    "        if hasattr(exp, \"base_values\"):\n",
    "            base_arr = np.array(exp.base_values).reshape(-1)\n",
    "            base_scalar = float(np.mean(base_arr))\n",
    "        else:\n",
    "            base_scalar = float(ffnn.predict(background_all).mean())\n",
    "        return shap_vals_arr, base_scalar\n",
    "\n",
    "    # Kernel fallback ‚Äî keep samples small, still works but slower\n",
    "    shap_vals_arr = np.array(explainer_all.shap_values(X_pos_s, nsamples=100))\n",
    "    if shap_vals_arr.ndim == 3:  # sometimes (1, n, d)\n",
    "        shap_vals_arr = shap_vals_arr[0]\n",
    "    try:\n",
    "        ev = explainer_all.expected_value\n",
    "        base_scalar = float(np.mean(ev)) if np.ndim(ev) else float(ev)\n",
    "    except Exception:\n",
    "        base_scalar = float(ffnn.predict(background_all).mean())\n",
    "    return shap_vals_arr, base_scalar\n",
    "\n",
    "# ============================\n",
    "# 3) SHAP per position (summary + a few force plots) ‚Äî with sampling\n",
    "# ============================\n",
    "N_POS_SAMPLES = 300  # cap per-position samples for speed; tune 100‚Äì500\n",
    "\n",
    "for pos in position_feature_map.keys():\n",
    "    # collect test indices for this position\n",
    "    pos_test_indices = []\n",
    "    for i, df_idx in enumerate(test_df_indices):\n",
    "        if df_idx is not None:\n",
    "            if get_position_from_row(df.iloc[df_idx]) == pos:\n",
    "                pos_test_indices.append(i)\n",
    "\n",
    "    if len(pos_test_indices) == 0:\n",
    "        print(f\"No test samples for position {pos}, skipping.\")\n",
    "        continue\n",
    "\n",
    "    # SAMPLE to speed up\n",
    "    if len(pos_test_indices) > N_POS_SAMPLES:\n",
    "        pos_test_indices = rng.choice(pos_test_indices, size=N_POS_SAMPLES, replace=False)\n",
    "\n",
    "    X_test_pos_s = X_test_s[pos_test_indices]\n",
    "\n",
    "    # Compute SHAP (fast)\n",
    "    shap_vals_pos, base_value_scalar = compute_shap_values_fast(X_test_pos_s)\n",
    "\n",
    "    # filter to position-relevant features\n",
    "    valid_features = [f for f in position_feature_map[pos] if f in features]\n",
    "    feature_indices = [features.index(f) for f in valid_features]\n",
    "    if len(feature_indices) == 0:\n",
    "        print(f\"No overlapping features for {pos}, skipping.\")\n",
    "        continue\n",
    "\n",
    "    filtered_shap  = shap_vals_pos[:, feature_indices]\n",
    "    filtered_Xtest = X_test_pos_s[:, feature_indices]\n",
    "\n",
    "    print(f\"\\n===================== üìå FFNN SHAP Global Summary for {pos} (n={filtered_Xtest.shape[0]}) =====================\")\n",
    "    shap.summary_plot(filtered_shap, filtered_Xtest, feature_names=valid_features, show=False)\n",
    "    plt.title(f\"FFNN ‚Äî SHAP Summary ({pos})\")\n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "\n",
    "    # a couple of force plots (max 3) ‚Äî cheap visual checks\n",
    "    # Optional: force plots for up to 3 samples (clean, no blank figure)\n",
    "    k = min(3, filtered_Xtest.shape[0])\n",
    "    for i in range(k):\n",
    "        try:\n",
    "            # Create a JS-based force plot (no extra matplotlib figure)\n",
    "            force_html = shap.force_plot(\n",
    "                base_value_scalar,\n",
    "                filtered_shap[i, :],\n",
    "                filtered_Xtest[i, :],\n",
    "                feature_names=valid_features\n",
    "            )\n",
    "    \n",
    "            # Display inline in Jupyter / Colab\n",
    "            from IPython.display import display, HTML\n",
    "            display(HTML(f\"<h4>FFNN ‚Äî SHAP Force ({pos}) ‚Ä¢ sample #{i+1}</h4>\"))\n",
    "            display(force_html)\n",
    "    \n",
    "        except Exception as e:\n",
    "            print(f\"(Force plot {i+1} for {pos} skipped due to: {e})\")\n",
    "\n"
   ]
  },
  {
