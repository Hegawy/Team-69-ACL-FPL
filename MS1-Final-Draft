    "    # Match-related stats (from match_features list)\n",
    "    'minutes': 21, # if >=60 = 2 pts. <60 = 1 pts. 0 = 0 pts\n",
    "    'goals_scored': 1, # 4 for fwd / 5 for mid / 6 for def and gw\n",
    "    'assists': 0, # 3 pts \n",
    "    'saves': 0, # every 3 saves = 1 pt\n",
    "    'goals_conceded': 0, # every 2 = -1 pt for def and gw\n",
    "    'clean_sheets': 0, # 4 for def and gw / 1 for mid\n",
