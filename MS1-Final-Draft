    "    \"DEF\": [\n",
    "        \"minutes\", \"clean_sheets\", \"goals_conceded\", \"bonus\", \"bps\",\n",
    "        \"influence\", \"threat\", \"yellow_cards\", \"red_cards\", \"own_goals\",\n",
    "        \"form\", \"value\", \"was_home\",\n",
    "        \"player_team_score\", \"opp_team_score\" , \"penalties_missed\"\n",
    "    ],\n",
    "    \n",
    "    \"GK\": [\n",
    "        \"minutes\", \"clean_sheets\", \"goals_conceded\", \"saves\", \"bonus\", \"bps\", \"own_goals\"\n",
    "        \"yellow_cards\", \"red_cards\",\n",
    "        \"form\", \"value\", \"was_home\",\n",
    "        \"player_team_score\", \"opp_team_score\" , \"penalties_missed\" , \"penalties_saved\"\n",
    "    ]\n",
    "}\n",
    "\n",
    "\n",
    "# Helper function to detect player position for a given test index\n",
    "def get_position_from_row(row):\n",
    "    if row[df.columns.get_loc(\"pos_FWD\")] == 1: return \"FWD\"\n",
    "    if row[df.columns.get_loc(\"pos_MID\")] == 1: return \"MID\"\n",
    "    if row[df.columns.get_loc(\"pos_DEF\")] == 1: return \"DEF\"\n",
    "    return \"GK\"\n"
   ]
