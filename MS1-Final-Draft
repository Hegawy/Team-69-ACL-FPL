    "df = pd.read_csv('/kaggle/input/fantasy-football/cleaned_merged_seasons.csv')\n",
    "\n",
    "# Normalize positions (GKP -> GK)\n",
    "position_col = next((c for c in df.columns if c.lower() in (\"position\",\"pos\",\"element_type\",\"player_position\")), None)\n",
    "if position_col:\n",
    "    df[position_col] = df[position_col].astype(str).str.upper().replace({\"GKP\":\"GK\"})\n",
    "    print(\"Position values:\", df[position_col].unique()[:20])\n"
